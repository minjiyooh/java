package t01;

public class Test5 {
	public static void main(String[] args) {
		int su = 30;
		System.out.printf("숫자 : %d\n", su);
		
		int su2 = 70;
		System.out.println("숫자2 : "  + su2);
		
		int su3, su4;
		su3 = 10;
		su4 = 20;
		System.out.println("su3 : " +  "su : 4" + su4);
	
		int su5 =5, su6 = 6;
		System.out.println("su5: " + su5 + ",su6: " + su6);
		
		int _su7 = 100, $su8 = 200 , su$9 = 300, main=400;
		System.out.println("_su7 : " + _su7 + " ,$su8: + " , su$9 : " + su$9" , "main:" +main);
		
	
	}
}
